---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Jacko's Blog</title>
	<link href="http://jpneylon.github.io/cv/atom.xml" rel="self"/>
	<link href="http://jpneylon.github.io/cv"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://jpneylon.github.io/cv</id>
	<author>
		<name>Jack Neylon</name>
		<email>j.p.neylon@gmail.com</email>
	</author>

	{% for patent in site.patents %}
		<entry>
			<title>{{ patent.title }}</title>
			<link href="http://jpneylon.github.io{{ patent.url }}"/>
			<updated>{{ patent.issued | date_to_xmlschema }}</updated>
			<id>http://jpneylon.github.io{{ patent.id }}</id>
			<content type="html">{{ patent.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
